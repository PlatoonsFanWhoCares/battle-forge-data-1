{
    "id": "british_and_welsh_kingdoms",
    "name": "British And Welsh Kingdoms (Brythonic)",
    "subfactions": {
        "south_welsh": {
            "name": "Southern Welsh (600-1100)",
            "phase": "activation",
            "id": "south_welsh",
            "description": "While seperated and hostile to one another, the southern Welsh kingdoms could easily unify to resist Anglo-Saxon aggression. They were heavily renown for their use of the bow, and became an inspiration to future generations.",
            "restricted": "true"
        }
    ,
        "powys": {
            "name": "Eastern Welsh (600-720)",
            "phase": "activation",
            "id": "powys",
            "description": "Before they were occupied by the Anglo-Saxons, these people made moderate use of the bow and were most successful in battle when raiding Anglo-Saxon settlers.",
            "restricted": "true"
        }
    ,
        "gwynedd": {
            "name": "Gwynedd (700-1034)",
            "phase": "activation",
            "id": "gwynedd",
            "description": "A kingdom within northern Wales, which was the most militarily successful against the English.",
            "restricted": "true"
        }
    ,
    "early_gwynedd": {
        "name": "Western Welsh (400-700)",
        "phase": "activation",
        "id": "early_gwynedd",
        "description": "The Welsh areas that still remain within 'Wales' in the modern day, with the greatest being the kingdom of Gwynedd. They are recorded as adopting guerilla tactics, but remaining trained with the throwing and thrusting spear.",
        "restricted": "true"
    },
        "early_strathclyde": {
            "name": "Northern Welsh (500-700)",
            "phase": "activation",
            "id": "early_strathclyde",
            "description": "South of the Picts and Scots, north of the Angles and Romano-British, lived four Celtic kingdoms. The strongest and best remembered was Strathclyde. These areas treasured their horses more than all others in the British Isles, and would thus work the hardest to have the finest horses and mounted warriors.",
            "restricted": "true"
        }
    ,
        "british": {
            "name": "British Successors (400-700)",
            "phase": "activation",
            "id": "british",
            "description": "These were the areas that were once Romano-British, but had either broken away from or forgotten most of the Roman ways. They would sadly, be conquered and subjugated by the Angles, Saxons, and Jutes.",
            "restricted": "true"
        }
        ,
        "dumnonia": {
            "name": "Dumnonia, Southwest Welsh (400-700)",
            "phase": "activation",
            "id": "dumnonia",
            "description": "This Celtic/Welsh area contained Dumnonia, which has lands suitable for the raising of horses and some of the remaining Romano 'stud-farms'. This kingdom also had the best access to trade with the continent and the surviving Romans.",
            "restricted": "true"
        },
        "anglian_kingdoms_heptarchy": {
            "name": "Anglian Kingdoms Heptarchy (600-700 AD)",
            "phase": "activation",
            "id": "anglian_kingdoms_heptarchy",
            "description": "The Angles/Anglos had their own kingdoms that were considered part of the 'Heptarchy', specifically East Anglia, Merica, Bernicia, and Deira. The 'North Angles' (Bernica and Deira) were unified into 'Northumbria' in 604 AD, (and despite converting after almost all the Saxon Kingdoms), they then had a 'golden age of religious and artistic works' from 650-750. ",
            "restricted": "true"
        }
    },
